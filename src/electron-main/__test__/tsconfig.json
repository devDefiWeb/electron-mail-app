{
  "extends": "./../tsconfig.json",
  "compilerOptions": {
    "outDir": "./../../../output/__test__/electron-main",
    "noEmitOnError": true,
    // TODO drop repeating "plugins" section (it currently doesn't seem to support inheritance/extending parent definition)
    "plugins": [
      {
        "transform": "./../../../scripts/typescript/webpack-build-env-transformer.ts"
      },
      {
        "transform": "@zerollup/ts-transform-paths"
      },
      {
        "transform": "ts-nameof",
        "type": "raw"
      }
    ]
  },
  "exclude": []
}
